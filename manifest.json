{
    "name": "dash - transient todo and notes app",
    "description": "With sync storage and themes",
    "version": "2.3",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+Z",
                "mac": "Command+Z"
            }
        }
    }
}